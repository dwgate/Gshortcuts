{
  "manifest_version": 2,
  "name": "#Tab Numbers",
  "description": "Tab numbering for faster cmd+# access",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },

  "icons": {},

  "permissions": [
    "background",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
