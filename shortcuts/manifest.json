{
  "manifest_version": 2,
  "name": "Gshortcuts",
  "description": "Extension shortcuts to google drive, gmail, etc",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },

  "icons": {},

  "permissions": [
    "background",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
